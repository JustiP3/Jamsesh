<h1>Create Profile</h1>

<% if @user.errors.any? %>
    <h4>"Errors:"</h4>
    <% @user.errors.full_messages.each do |msg| %>
        <p><%= msg %></p>
    <% end %>
<% end %>

<% if flash[:message] %>
    <h4>"Errors:"</h4>
    <p><% flash[:message] %></p>
<% end %>


<%= render 'form' %>
